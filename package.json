{
    "name": "vscode-http-client",
    "displayName": "HTTP Client",
    "description": "Simple way to do HTTP requests.",
    "version": "0.0.4",
    "publisher": "mkloubert",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "HTTP",
        "Client",
        "REST",
        "API",
        "Requests",
        "multi-root ready"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.http.client.newRequest",
                "title": "New request ...",
                "category": "HTTP Client"
            },
            {
                "command": "extension.http.client.newRequestForActiveEditor",
                "title": "Send editor content...",
                "category": "HTTP Client"
            }
        ],
        "configuration": {
            "properties": {
                "http.client": {
                    "type": "object",
                    "scope": "resource",
                    "properties": {
                        "openOnStartup": {
                            "description": "Opens a new request on startup or not.",
                            "type": "boolean",
                            "default": false
                        }
                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "icon": "icon.png",
    "badges": [
        {
            "description": "PayPal Donate",
            "url": "https://img.shields.io/badge/Donate-PayPal-green.svg",
            "href": "https://paypal.me/MarcelKloubert"
        }
    ],
    "author": {
        "name": "Marcel Joachim Kloubert"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mkloubert/vscode-http-client"
    },
    "bugs": {
        "url": "https://github.com/mkloubert/vscode-http-client/issues"
    },
    "readmeFilename": "README.md",
    "devDependencies": {
        "@types/lodash": "^4.14.108",
        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "header-case-normalizer": "^1.0.3",
        "lodash": "^4.17.10",
        "mime-types": "^2.1.18",
        "vscode-helpers": "^1.10.0"
    }
}
